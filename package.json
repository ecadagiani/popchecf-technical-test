    "@testing-library/react": "13.3.0",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/jest": "27.4.1",
    "@types/node": "16.11.7",
    "@types/react": "18.0.13",
    "@types/react-dom": "18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "~5.24.0",
