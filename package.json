{
  "name": "fullstack",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx:run": "nx run",
    "start": "nx serve",
    "start:all": "nx run-many --target=serve --projects=frontend,backend",
    "start:backend": "nx serve backend --host=0.0.0.0 --port=4311",
    "start:frontend": "nx serve frontend --host=0.0.0.0",
    "backend:graphql-codegen": "nx run backend:graphql-codegen && nx format:write",
    "orm:drop-schema": "nx run backend:drop-schema",
    "build": "nx build",
    "test": "nx test",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down --remove-orphans",
    "typeorm-cli": "ts-node --project tsconfig.base.json -O '{\"module\": \"commonjs\", \"experimentalDecorators\": true}' -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "ts-node --project tsconfig.base.json -O '{\"module\": \"commonjs\", \"experimentalDecorators\": true}' -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d apps/backend/src/app/services/Database.ts",
    "migration:revert": "ts-node --project tsconfig.base.json -O '{\"module\": \"commonjs\", \"experimentalDecorators\": true}' -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d apps/backend/src/app/services/Database.ts",
    "migration:generate": "ts-node --project tsconfig.base.json -O '{\"module\": \"commonjs\", \"experimentalDecorators\": true}' -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d apps/backend/src/app/services/Database.ts"
  },
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
